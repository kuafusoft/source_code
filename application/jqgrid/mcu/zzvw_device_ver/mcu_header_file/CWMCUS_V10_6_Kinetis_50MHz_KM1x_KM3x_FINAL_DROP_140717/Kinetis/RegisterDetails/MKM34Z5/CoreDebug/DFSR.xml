<register-details>
  <name>DFSR</name>
  <bitrange>31:0</bitrange>
  <reset-value>0</reset-value>
  <description>Debug Fault Status Register</description>
  <bitfields>
    <bitfield>
      <name>HALTED</name>
      <bitrange>0</bitrange>
      <format>binary</format>
      <access>readwrite</access>
      <description>HALTED bit. Indicates a debug event generated by either:&#10;- a C_HALT or C_STEP request, triggered by a write to the DHCSR&#10;- a step request triggered by setting DEMCR.MON_STEP to 1</description>
      <values>
        <value>
          <value>0b0</value>
          <description>No active halt request debug event.</description>
        </value>
        <value>
          <value>0b1</value>
          <description>Halt request debug event active.</description>
        </value>
      </values>
    </bitfield>
    <bitfield>
      <name>BKPT</name>
      <bitrange>1</bitrange>
      <format>binary</format>
      <access>readwrite</access>
      <description>BKPT bit. Indicates a debug event generated by BKPT instruction execution or a breakpoint match in&#10;FPB.</description>
      <values>
        <value>
          <value>0b0</value>
          <description>No current breakpoint debug event.</description>
        </value>
        <value>
          <value>0b1</value>
          <description>At least one current breakpoint debug event.</description>
        </value>
      </values>
    </bitfield>
    <bitfield>
      <name>DWTTRAP</name>
      <bitrange>2</bitrange>
      <format>binary</format>
      <access>readwrite</access>
      <description>DWTTRAP bit. Indicates a debug event generated by the DWT.</description>
      <values>
        <value>
          <value>0b0</value>
          <description>No current debug events generated by the DWT.</description>
        </value>
        <value>
          <value>0b1</value>
          <description>At least one current debug event generated by the DWT.</description>
        </value>
      </values>
    </bitfield>
    <bitfield>
      <name>VCATCH</name>
      <bitrange>3</bitrange>
      <format>binary</format>
      <access>readwrite</access>
      <description>VCATCH bit. Indicates triggering of a Vector catch.&#10;The corresponding FSR shows the primary cause of the exception.</description>
      <values>
        <value>
          <value>0b0</value>
          <description>No Vector catch triggered.</description>
        </value>
        <value>
          <value>0b1</value>
          <description>Vector catch triggered.</description>
        </value>
      </values>
    </bitfield>
    <bitfield>
      <name>EXTERNAL</name>
      <bitrange>4</bitrange>
      <format>binary</format>
      <access>readwrite</access>
      <description>EXTERNAL bit. Indicates a debug event generated because of the assertion of EDBGRQ.</description>
      <values>
        <value>
          <value>0b0</value>
          <description>No EDBGRQ debug event.</description>
        </value>
        <value>
          <value>0b1</value>
          <description>EDBGRQ debug event.</description>
        </value>
      </values>
    </bitfield>
