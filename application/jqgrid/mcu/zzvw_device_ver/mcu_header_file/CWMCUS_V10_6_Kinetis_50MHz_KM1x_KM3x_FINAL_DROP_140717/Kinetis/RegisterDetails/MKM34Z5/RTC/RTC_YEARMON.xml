<?xml version="1.0" encoding="UTF-8" standalone="no" ?>
<!DOCTYPE register-details-file>
<register-details-file xmlns="http://www.freescale.com/schema/ddd/1.0/detail" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.freescale.com/schema/ddd/1.0/detail RegisterDetails.xsd">

<register-details>
  <name>RTC_YEARMON</name>
  <bitrange>15:0</bitrange>
  <reset-value>0x1</reset-value>
  <description>RTC Year and Month Counters Register</description>
  <bitfields>
    <bitfield>
      <name>MON_CNT</name>
      <bitrange>3:0</bitrange>
      <format>binary</format>
      <access>readwrite</access>
      <description>These bits give the value of the Months Counter . Valid Values are:</description>
      <values>
        <value>
          <value>0b0</value>
          <description>Illegal Value</description>
        </value>
        <value>
          <value>0b1</value>
          <description>January</description>
        </value>
        <value>
          <value>0b10</value>
          <description>October</description>
        </value>
        <value>
          <value>0b11</value>
          <description>November</description>
        </value>
      </values>
    </bitfield>
    <bitfield>
      <name/>
      <bitrange>7:4</bitrange>
      <format>binary</format>
      <access>read</access>
      <description>Reserved bits .</description>
    </bitfield>
    <bitfield>
      <name>YROFST</name>
      <bitrange>15:8</bitrange>
      <format>binary</format>
      <access>readwrite</access>
      <description>Year Offset Count Value</description>
    </bitfield>
  </bitfields>
</register-details>

</register-details-file>
