<?xml version="1.0" encoding="UTF-8" standalone="no" ?>
<!DOCTYPE register-details-file>
<register-details-file xmlns="http://www.freescale.com/schema/ddd/1.0/detail" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.freescale.com/schema/ddd/1.0/detail RegisterDetails.xsd">

<register-details>
  <name>CRC_DATA</name>
  <bitrange>31:0</bitrange>
  <reset-value>0xFFFFFFFF</reset-value>
  <description>CRC Data register</description>
  <bitfields>
    <bitfield>
      <name>LL</name>
      <bitrange>7:0</bitrange>
      <format>binary</format>
      <access>readwrite</access>
      <description>CRC Low Lower Byte</description>
    </bitfield>
    <bitfield>
      <name>LU</name>
      <bitrange>15:8</bitrange>
      <format>binary</format>
      <access>readwrite</access>
      <description>CRC Low Upper Byte</description>
    </bitfield>
    <bitfield>
      <name>HL</name>
      <bitrange>23:16</bitrange>
      <format>binary</format>
      <access>readwrite</access>
      <description>CRC High Lower Byte</description>
    </bitfield>
    <bitfield>
      <name>HU</name>
      <bitrange>31:24</bitrange>
      <format>binary</format>
      <access>readwrite</access>
      <description>CRC High Upper Byte</description>
    </bitfield>
  </bitfields>
</register-details>

</register-details-file>
