<!DOCTYPE register-details-file>
<register-details-file xmlns="http://www.freescale.com/schema/ddd/1.0/detail" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.freescale.com/schema/ddd/1.0/detail RegisterDetails.xsd">

<register-details>
  <name>RTC_DST_MONTH</name>
  <bitrange>15:0</bitrange>
  <reset-value>0</reset-value>
  <description>RTC Daylight Saving Month Register</description>
  <bitfields>
    <bitfield>
      <name>DST_END_MONTH</name>
      <bitrange>3:0</bitrange>
      <format>binary</format>
      <access>readwrite</access>
      <description>Daylight Saving Time (DST) Month End Value.</description>
    </bitfield>
    <bitfield>
      <name/>
      <bitrange>7:4</bitrange>
      <format>binary</format>
      <access>read</access>
      <description>Reserved bits</description>
    </bitfield>
    <bitfield>
      <name>DST_START_MONTH</name>
      <bitrange>11:8</bitrange>
      <format>binary</format>
      <access>readwrite</access>
      <description>Daylight Saving Time (DST) Month Start Value.</description>
    </bitfield>
    <bitfield>
      <name/>
      <bitrange>15:12</bitrange>
      <format>binary</format>
      <access>read</access>
      <description>Reserved bits</description>
    </bitfield>
  </bitfields>
</register-details>

</register-details-file>
