<?xml version="1.0" encoding="UTF-8" standalone="no" ?>
<!DOCTYPE register-details-file>
<register-details-file xmlns="http://www.freescale.com/schema/ddd/1.0/detail" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.freescale.com/schema/ddd/1.0/detail RegisterDetails.xsd">

<register-details>
  <name>LCD_FDSR</name>
  <bitrange>31:0</bitrange>
  <reset-value>0</reset-value>
  <description>LCD Fault Detect Status Register</description>
  <bitfields>
    <bitfield>
      <name>FDCNT</name>
      <bitrange>7:0</bitrange>
      <format>binary</format>
      <access>read</access>
      <description>Fault Detect Counter</description>
      <values>
        <value>
          <value>0b0</value>
          <description>No &quot;one&quot; samples.</description>
        </value>
        <value>
          <value>0b1</value>
          <description>1 &quot;one&quot; samples.</description>
        </value>
      </values>
    </bitfield>
    <bitfield>
      <name/>
      <bitrange>14:8</bitrange>
      <format>binary</format>
      <access>read</access>
      <description>Reserved</description>
    </bitfield>
    <bitfield>
      <name>FDCF</name>
      <bitrange>15</bitrange>
      <format>binary</format>
      <access>readwrite</access>
      <description>Fault Detection Complete Flag</description>
      <values>
        <value>
          <value>0b0</value>
          <description>Fault detection is not completed.</description>
        </value>
        <value>
          <value>0b1</value>
          <description>Fault detection is completed.</description>
        </value>
      </values>
    </bitfield>
    <bitfield>
      <name/>
      <bitrange>31:16</bitrange>
      <format>binary</format>
      <access>read</access>
      <description>Reserved</description>
    </bitfield>
  </bitfields>
</register-details>

</register-details-file>
