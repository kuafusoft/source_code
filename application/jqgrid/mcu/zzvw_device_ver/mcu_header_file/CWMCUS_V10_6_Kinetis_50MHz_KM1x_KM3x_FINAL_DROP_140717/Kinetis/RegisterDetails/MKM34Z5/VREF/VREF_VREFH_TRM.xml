<?xml version="1.0" encoding="UTF-8" standalone="no" ?>
<!DOCTYPE register-details-file>
<register-details-file xmlns="http://www.freescale.com/schema/ddd/1.0/detail" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.freescale.com/schema/ddd/1.0/detail RegisterDetails.xsd">

<register-details>
  <name>VREF_VREFH_TRM</name>
  <bitrange>7:0</bitrange>
  <description>VREF Trim Register</description>
  <bitfields>
    <bitfield>
      <name>TRIM</name>
      <bitrange>5:0</bitrange>
      <format>binary</format>
      <access>readwrite</access>
      <description>Trim bits</description>
      <values>
        <value>
          <value>0b000000</value>
          <description>Min</description>
        </value>
        <value>
          <value>0b111111</value>
          <description>Max</description>
        </value>
      </values>
    </bitfield>
    <bitfield>
      <name>CHOPEN</name>
      <bitrange>6</bitrange>
      <format>binary</format>
      <access>readwrite</access>
      <description>Chop oscillator enable. When set, internal chopping operation is enabled and the internal analog offset will be minimized.</description>
      <values>
        <value>
          <value>0b0</value>
          <description>Chop oscillator is disabled.</description>
        </value>
        <value>
          <value>0b1</value>
          <description>Chop oscillator is enabled.</description>
        </value>
      </values>
    </bitfield>
    <bitfield>
      <name/>
      <bitrange>7</bitrange>
      <format>binary</format>
      <access>read</access>
      <description>Upon reset this value is loaded with a factory trim value.</description>
    </bitfield>
  </bitfields>
</register-details>

</register-details-file>
