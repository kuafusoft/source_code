        <value>
          <value>0b0</value>
          <description>PLL has not lost lock since COARSE_LOLS was last cleared.</description>
        </value>
        <value>
          <value>0b1</value>
          <description>PLL has lost lock since COARSE_LOLS was last cleared.</description>
        </value>
      </values>
    </bitfield>
