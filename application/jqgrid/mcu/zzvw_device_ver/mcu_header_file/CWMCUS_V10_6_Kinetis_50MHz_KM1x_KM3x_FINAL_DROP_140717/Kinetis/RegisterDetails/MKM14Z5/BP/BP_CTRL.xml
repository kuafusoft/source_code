<register-details>
  <name>BP_CTRL</name>
  <bitrange>31:0</bitrange>
  <reset-value>0x30</reset-value>
  <description>FlashPatch Control Register</description>
  <bitfields>
    <bitfield>
      <name>ENABLE</name>
      <bitrange>0</bitrange>
      <format>binary</format>
      <access>readwrite</access>
      <description>Enable bit for the FPB.&#10;A Power-on reset clears this bit to 0.</description>
      <values>
        <value>
          <value>0b0</value>
          <description>FPB disabled.</description>
        </value>
        <value>
          <value>0b1</value>
          <description>FPB enabled.</description>
        </value>
      </values>
    </bitfield>
    <bitfield>
      <name>KEY</name>
      <bitrange>1</bitrange>
      <format>binary</format>
      <access>readwrite</access>
      <description>KEY bit. On any write to FP_CTRL, the FPB unit ignores the write unless this bit is 1. This bit is RAZ.</description>
    </bitfield>
